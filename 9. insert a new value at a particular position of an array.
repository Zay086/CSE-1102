#include <stdio.h>

int main() {
    int n,t,p,temp;
    scanf("%d",&n);
    int arr[n+1];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
        
    }
    
    //printf("The value you want to add and position");
    scanf("%d %d",&t,&p);
    temp=arr[p-1];
    arr[p-1]=t;
    for(int i=0;i<=n;i++)
    {
        if(n-i==p)
        {
            arr[n-i]=temp;
            break;
        }
        arr[n-i]=arr[n-1-i];
        
        
    }
     for(int i=0;i<=n;i++)
    {
        printf("%d ",arr[i]);
        
    }
    
    
}
