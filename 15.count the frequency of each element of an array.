#include <stdio.h>

int main() {
    int n,p=1,temp;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
        
    }
    for(int i=0;i<n;i++)
    {
        p=1;
        for(int j=i+1;j<n;j++)
        {
            if(arr[i]==arr[j])
            {
                p++;
            }
        }
        if(p>1)
        {
            printf("%d has duplicates of %d\n",arr[i],p);
        }
    }
    
    
    
    
    
}
