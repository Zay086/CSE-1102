#include <stdio.h>

int main() {
    int n,f,i,t=0;
    scanf("%d",&n);
    int arr[n];
    for( i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
        
    }
    scanf("%d",&f);
    for( i=0;i<n;i++)
    {
        if(arr[i]==f)
        {
            printf("%d at %d",f,i+1);
            return 0;
        }
        
    }
    printf("Not found");
    
}
