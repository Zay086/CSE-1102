#include <stdio.h>

int main() {
    int n,t;
    scanf("%d",&n);
    int arr[n+1];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
        
    }
    
    printf("The value you want to add at last");
    scanf("%d",&t);
    
    arr[n]=t;
    for(int i=0;i<=n;i++)
    {
        printf("%d ",arr[i]);
        
    }
    
    
}
